      <Textbox Name="Textbox30">
        <CanGrow>true</CanGrow>
        <KeepTogether>true</KeepTogether>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>Nos dirigimos a Usted con el propósito de manifestar lo siguiente:</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value xml:space="preserve">                                                   </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>                                                     A la fecha, su afianzado el Señor </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Parameters!cNombreTitular.Value</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value> no ha cumplido con honrar el pago de </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Parameters!nCuotasAtraso.Value</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value> cuotas impagas las mismas que ascienden a la suma de </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Parameters!nMontoDeuda.Value</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>, registrando </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>=Parameters!nDiasAtraso.Value</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value> días de atraso, por lo cual se le requiere para que en su calidad de </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>Fiador Solidario</Value>
                <Style>
                  <FontStyle>Italic</FontStyle>
                  <FontFamily />
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value> efectúe el pago total de la deuda atrasada en forma inmediata.</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value />
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>                                                    En caso de no regularizar el pago de la deuda atrasada se procederá a </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>solicitar el inicio de las acciones judiciales de cobranza</Value>
                <Style>
                  <FontStyle>Italic</FontStyle>
                  <FontFamily />
                  <FontSize>11pt</FontSize>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value> ante los órganos jurisdiccionales competentes, las mismas que involucrarán el incremento significativo de su obligación crediticia debido a los altos costos acaecidos por los aranceles judiciales y honorarios del estudio jurídico que esté a cargo de estas acciones; por lo que dependerá de su afianzado y Usted, evitar el inicio de estas gestiones judiciales.</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value />
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>                                                    Por ello, se le remite el presente </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>REQUERIMIENTO PERENTORIO DE PAGO </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontSize>12pt</FontSize>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>para que dentro del plazo de </Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontWeight>Normal</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
                </Style>
              </TextRun>
              <TextRun>
                <Value>24 HORAS</Value>
                <Style>
                  <FontStyle>Normal</FontStyle>
                  <FontFamily />
                  <FontSize>12pt</FontSize>
                  <FontWeight>Bold</FontWeight>
                  <TextDecoration>None</TextDecoration>
                  <Color>#000000</Color>
