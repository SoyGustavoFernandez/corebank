									<TextRun>
										<Value>RESUMEN DE ALGUNAS CONDICIONES CONTRACTUALES RELEVANTES PARA LAS PARTES:</Value>
										<Style>
											<FontStyle>Normal</FontStyle>
											<FontSize>9pt</FontSize>
